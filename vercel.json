{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 60,
      "includeFiles": "node_modules/puppeteer-extra/**,node_modules/puppeteer-extra-plugin-stealth/**,node_modules/puppeteer-extra-plugin-user-preferences/**,node_modules/puppeteer-extra-plugin-user-data-dir/**,node_modules/@sparticuz/chromium/**,node_modules/puppeteer-core/**"
    }
  }
}